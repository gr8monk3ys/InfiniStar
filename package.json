{
  "name": "next-template",
  "version": "0.0.2",
  "private": true,
  "packageManager": "bun@1.3.8",
  "scripts": {
    "dev": "next dev",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "preview": "next build && next start",
    "typecheck": "tsc --noEmit",
    "format:write": "prettier --write \"**/*.{ts,tsx,mdx}\" --cache",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:auth": "bash -lc 'set -a; [ -f .env.ci.example ] && source .env.ci.example; [ -f .env.local ] && source .env.local; set +a; playwright test e2e/auth.spec.ts e2e/conversations.spec.ts e2e/profile.spec.ts e2e/ai-chat.spec.ts'",
    "test:e2e:redirects": "bash -lc 'set -a; [ -f .env.ci.example ] && source .env.ci.example; [ -f .env.local ] && source .env.local; set +a; SKIP_CLERK_AUTH_HANDSHAKE=0 E2E_ASSERT_AUTH_REDIRECTS=true playwright test e2e/auth.spec.ts e2e/conversations.spec.ts e2e/profile.spec.ts -g redirect'",
    "test:e2e:live-ai": "bash -lc 'set -a; [ -f .env.ci.example ] && source .env.ci.example; [ -f .env.local ] && source .env.local; set +a; E2E_ASSERT_AUTH_REDIRECTS=true E2E_RUN_LIVE_AI=true playwright test e2e/ai-chat.spec.ts'",
    "test:load:smoke": "node scripts/load_smoke.mjs",
    "ops:stripe:webhook:verify": "node scripts/verify_stripe_webhook_staging.mjs",
    "ops:sentry:alerts:audit": "node scripts/audit_sentry_alerts.mjs",
    "ops:ai:unit-economics": "node scripts/unit_economics.mjs",
    "ops:db:backup-restore:drill": "bash scripts/db_backup_restore_drill.sh",
    "ci:release:gate": "bash scripts/release_gate.sh",
    "ci:release:gate:full": "RUN_E2E=1 RUN_LOAD_SMOKE=1 bash scripts/release_gate.sh",
    "ci:build": "bash -lc 'set -a; [ -f .env.ci.example ] && source .env.ci.example; set +a; SKIP_ENV_VALIDATION=1 bun run build'",
    "seed": "ts-node --compiler-options {\\\"module\\\":\\\"CommonJS\\\"} prisma/seed.ts",
    "postinstall": "prisma generate",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,json,md,mdx}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.72.1",
    "@clerk/nextjs": "^6.37.3",
    "@headlessui/react": "^2.2.9",
    "@hookform/resolvers": "^5.2.2",
    "@neondatabase/serverless": "^0.10.4",
    "@prisma/adapter-neon": "^7.3.0",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-toast": "^1.2.15",
    "@sentry/nextjs": "^8.49.0",
    "@t3-oss/env-nextjs": "^0.13.10",
    "@types/lodash": "^4.17.23",
    "@types/react-select": "^5.0.1",
    "axios": "^1.13.5",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "ioredis": "^5.9.2",
    "lodash": "^4.17.23",
    "lucide-react": "^0.563.0",
    "next": "^16.1.6",
    "next-cloudinary": "^6.17.5",
    "next-themes": "^0.4.6",
    "otplib": "^13.2.1",
    "pino": "^9.14.0",
    "postmark": "^4.0.5",
    "prism-react-renderer": "^2.4.1",
    "pusher": "^5.3.2",
    "pusher-js": "^8.4.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.44.3",
    "react-hot-toast": "^2.6.0",
    "react-icons": "^5.5.0",
    "react-select": "^5.9.0",
    "react-spinners": "^0.17.0",
    "recharts": "^3.7.0",
    "sharp": "^0.34.5",
    "stripe": "^20.3.0",
    "svix": "^1.84.1",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.6",
    "ua-parser-js": "^2.0.9",
    "ws": "^8.18.3",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.7.0",
    "@playwright/test": "^1.58.1",
    "@prisma/client": "^7.3.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "^3.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.0",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@types/ua-parser-js": "^0.7.39",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vercel/edge": "^1.2.2",
    "autoprefixer": "^10.4.24",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "lint-staged": "^16.2.7",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "prisma": "^7.3.0",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "@babel/runtime": "^7.28.6",
    "@types/node-fetch": "2.6.13",
    "form-data": "^4.0.5"
  }
}
